pkgname=man-pages
pkgver=5.05
vcs=git
gittag=man-pages-${pkgver}
relmon_id=1883

build() {
    :
}

package() {
    make prefix=${pkgdir}/usr install
    # these are included in shadow
    rm -f ${pkgdir}/usr/share/man/man5/passwd.5
    rm -f ${pkgdir}/usr/share/man/man3/getspnam.3
    rm -f ${pkgdir}/usr/share/man/man5/attr.5
}
