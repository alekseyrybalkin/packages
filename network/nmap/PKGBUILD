pkgname=nmap
pkgver=7.80
vcs=git
#NOTE manuaul tags
gittag=${pkgver}
relmon_id=2096

build() {
    ./configure --prefix=/usr \
        --without-ndiff \
        --without-nping \
        --without-zenmap \
        --without-liblua
    make
}

package() {
    MAKEFLAGS=
    make DESTDIR=${pkgdir} install
}
