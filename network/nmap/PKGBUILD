pkgname=nmap
pkgver=7.90
vcs=git
# f05296d7f7f7d735e5ccf0f2dca07bc2aa3b7ae8
gittag=jinni-${pkgver}
relmon_id=2096

build() {
    ./configure --prefix=/usr \
        --without-ndiff \
        --without-nping \
        --without-zenmap \
        --without-liblua
    make
}

package() {
    MAKEFLAGS=
    make DESTDIR=${pkgdir} install
}
