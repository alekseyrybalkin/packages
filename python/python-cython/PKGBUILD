vcs=git
vcs_pkgname=cython
gittag=${pkgver}
relmon_id=12679

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
