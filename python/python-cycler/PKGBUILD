pkgname=python-cycler
pkgver=0.10.0
vcs=git
vcs_pkgname=cycler
gittag=v${pkgver}
relmon_id=11552

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
