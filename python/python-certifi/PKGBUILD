pkgname=python-certifi
_pkgver=2020.04.05.1
pkgver=${_pkgver//\.0/\.}
vcs=git
gittag=${_pkgver}
relmon_id=7995

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
