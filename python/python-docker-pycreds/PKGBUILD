pkgname=python-docker-pycreds
pkgver=0.4.0
vcs=git
vcs_pkgname=dockerpy-creds
gittag=${pkgver}
relmon_id=12091

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
