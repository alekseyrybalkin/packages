pkgname=python-jedi
pkgver=0.17.2
vcs=git
vcs_pkgname=jedi
gittag=v${pkgver}
relmon_id=3893

build() {
    git clone `find_vcs_repo typeshed` jedi/third_party/typeshed
    git clone `find_vcs_repo django-stubs` jedi/third_party/django-stubs
    git submodule update --init

    python-build
}

package() {
    python-package
}
