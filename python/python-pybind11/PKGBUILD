pkgname=python-pybind11
pkgver=2.5.0
vcs=git
vcs_pkgname=pybind11
gittag=v${pkgver}
relmon_id=13384

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
