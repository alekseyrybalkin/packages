pkgname=python-werkzeug
pkgver=1.0.1
vcs=git
vcs_pkgname=werkzeug
gittag=${pkgver}
relmon_id=4092

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
