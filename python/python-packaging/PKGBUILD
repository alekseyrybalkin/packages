pkgname=python-packaging
pkgver=20.4
vcs=git
vcs_pkgname=packaging
gittag=${pkgver}
relmon_id=11718

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
