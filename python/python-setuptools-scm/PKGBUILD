pkgname=python-setuptools-scm
pkgver=4.1.2
vcs=git
vcs_pkgname=setuptools_scm
gittag=v${pkgver}
relmon_id=7874

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
