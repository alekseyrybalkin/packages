pkgname=python-ptyprocess
pkgver=0.6.0
vcs=git
vcs_pkgname=ptyprocess
gittag=${pkgver}
relmon_id=6447

build() {
    cp ../setup.py .
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
