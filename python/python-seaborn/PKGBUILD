vcs=git
vcs_pkgname=seaborn
gittag=v${pkgver}
relmon_id=7691

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
