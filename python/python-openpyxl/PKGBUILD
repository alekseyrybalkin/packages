pkgname=python-openpyxl
pkgver=3.0.3
vcs=mercurial
vcs_pkgname=openpyxl
hgtag=${pkgver}
relmon_id=8098

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
