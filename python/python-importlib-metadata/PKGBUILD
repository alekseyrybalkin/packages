pkgname=python-importlib-metadata
pkgver=1.6.0
vcs=git
vcs_pkgname=importlib_metadata
gittag=v${pkgver}

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
