pkgname=python-ipython
pkgver=7.15.0
vcs=git
vcs_pkgname=ipython
gittag=${pkgver}
relmon_id=1399

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
