pkgname=python-pycparser
pkgver=2.20
vcs=git
vcs_pkgname=pycparser
gittag=release_v${pkgver}
relmon_id=8175

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
