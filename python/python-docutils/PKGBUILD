pkgname=python-docutils
vcs_pkgname=docutils
pkgver=0.16
vcs=git
gittag=docutils-${pkgver}
relmon_id=3849

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
