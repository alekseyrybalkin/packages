pkgname=python-dulwich
pkgver=0.20.2
vcs=git
vcs_pkgname=dulwich
gittag=dulwich-${pkgver}
relmon_id=6077

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
