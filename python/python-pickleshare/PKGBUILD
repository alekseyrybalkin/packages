pkgname=python-pickleshare
pkgver=0.7.5
vcs=git
vcs_pkgname=pickleshare
gittag=${pkgver}
relmon_id=12037

build() {
    :
}

package() {
    pip install \
        --no-deps \
        --no-build-isolation \
        --ignore-installed \
        --compile \
        --prefix=/usr \
        --root=${pkgdir} .
}
