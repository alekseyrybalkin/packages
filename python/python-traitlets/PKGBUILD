pkgname=python-traitlets
pkgver=4.3.3
vcs=git
vcs_pkgname=traitlets
gittag=${pkgver}
relmon_id=12211

build() {
    # TODO: remove in the future
    git clone --branch 0.2.0 `find_vcs_repo ipython_genutils`
}

package() {
    # TODO: remove in the future
    cd ipython_genutils
    python setup.py install --prefix=/usr --root=${pkgdir} --optimize=0
    cd ../
    python setup.py install --prefix=/usr --root=${pkgdir} --optimize=0
}
